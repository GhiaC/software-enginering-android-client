<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:circle="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.szugyi.circlemenu.view.CircleImageView
        android:id="@+id/charity"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:elevation="6dp"
        android:src="@drawable/charityicon2"
        circle:name="calendar" />

    <com.szugyi.circlemenu.view.CircleImageView
        android:id="@+id/shops"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:elevation="6dp"
        android:src="@drawable/shopsicon"
        circle:name="calendar" />

    <com.szugyi.circlemenu.view.CircleImageView
        android:id="@+id/carService"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:elevation="6dp"
        android:src="@drawable/carservice3"
        circle:name="calendar" />

    <com.szugyi.circlemenu.view.CircleImageView
        android:id="@+id/homeService"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:elevation="6dp"
        android:src="@drawable/homeservice"
        circle:name="calendar" />

    <com.szugyi.circlemenu.view.CircleImageView
        android:id="@+id/health"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:elevation="6dp"
        android:src="@drawable/hospital"
        circle:name="calendar" />


</merge>